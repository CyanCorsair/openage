#include "engine/unit.h"
append_functor_to_changelist(String definitionstuff) {
	parse_definitistuff

	changelist<Unit>.append(functor_to_set_name_to_string(Unit &u) {
		u.hp = gschicht;
	})
	changelist<Unit>.append(functor_to_set_name_to_string(Unit &u) {
		u.hp *= gschicht;
	})
}

